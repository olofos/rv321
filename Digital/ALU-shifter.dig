          <testData>
            <dataString>BUS-CLK ~{L}A SH-RESET SH-~OUTPUT SH-IN       SH-LENIN       SH-LOADLEN SH-OUT    DLEN

loop (rep,32)

# SRA

let A = random(1&lt;&lt;32);
let L = 7;
let R = (A &gt;&gt; (31 - L));

# Load shift length

loop (i,32)
0   1    0         1          0           (L&gt;&gt;i&amp;1)       1          X         X
1   1    0         1          0           (L&gt;&gt;i&amp;1)       1          X         X
end loop

# Latch length
0   1    1         1          0           0              0          X         X
0   1    0         1          0           0              0          X         X

# Load argument
loop (i,32)
0   1    0         1          (A&gt;&gt;i&amp;1)    0              0          X         X
1   1    0         1          (A&gt;&gt;i&amp;1)    0              0          X         X
end loop

# Output result
loop (i,7)
0   1    0         0          0           0              0          (R&gt;&gt;i&amp;1)  X
1   1    0         0          0           0              0          X         X
end loop

loop (i,32-7)
0   1    0         0          0           0              0          (A&gt;&gt;31&amp;1) X
1   1    0         0          0           0              0          X         X
end loop

# SLL

let A = random(1&lt;&lt;32);
let dL = random(32);

let L = 32 + dL;
let R = (A &lt;&lt; (L - 31));
loop (i,32)
0   0    0         1          0           (L&gt;&gt;i&amp;1)       1          X         X
1   0    0         1          0           (L&gt;&gt;i&amp;1)       1          X         X
end loop
0   0    1         1          0           0              0          X         X
0   0    0         1          0           0              0          X         X
loop (i,32)
0   0    0         1          (A&gt;&gt;i&amp;1)    0              0          X         X
1   0    0         1          (A&gt;&gt;i&amp;1)    0              0          X         X
end loop
loop (i,32)
0   0    0         0          0           0              0          (R&gt;&gt;i&amp;1)  X
1   0    0         0          0           0              0          X         X
end loop

# SRL

let L = dL;
let R = (A &gt;&gt; (31 - L));

loop (i,32)
0   0    0         1          0           (L&gt;&gt;i&amp;1)       1          X         X
1   0    0         1          0           (L&gt;&gt;i&amp;1)       1          X         X
end loop
0   0    1         1          0           0              0          X         X
0   0    0         1          0           0              0          X         X
loop (i,32)
0   0    0         1          (A&gt;&gt;i&amp;1)    0              0          X         X
1   0    0         1          (A&gt;&gt;i&amp;1)    0              0          X         X
end loop
loop (i,32)
0   0    0         0          0           0              0          (R&gt;&gt;i&amp;1)  X
1   0    0         0          0           0              0          X         X
end loop

end loop
</dataString>
