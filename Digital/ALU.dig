            <dataString>BUS-CLK S         A        B        N ALU-~RESET ALU-AUX   OUT           FLAG DLEN DSUM

loop (rep,32)

let A = random(1&lt;&lt;32);
let B = random(1&lt;&lt;32);

let ADD = 0;
let OR  = 1;
let XOR = 2;
let AND = 3;

# A+B

0       (ADD)     0        0        0 1          0         X             X    X    X
1       (ADD)     0        0        0 0          0         X             X    X    X

loop (i,32)
0       (ADD)     (A&gt;&gt;i&amp;1) (B&gt;&gt;i&amp;1) 0 1          0         ((A+B)&gt;&gt;i&amp;1)  X    X    X
1       (ADD)     (A&gt;&gt;i&amp;1) (B&gt;&gt;i&amp;1) 0 1          0         X             X    X    X
end loop

#A-B

0       (ADD)     0        0        1 1          0         X             X    X    X
1       (ADD)     0        0        1 0          0         X             X    X    X

loop (i,32)
0       (ADD)     (A&gt;&gt;i&amp;1) (B&gt;&gt;i&amp;1) 1 1          0         ((A-B)&gt;&gt;i&amp;1)  X    X    X
1       (ADD)     (A&gt;&gt;i&amp;1) (B&gt;&gt;i&amp;1) 1 1          0         X             X    X    X
end loop

#A|B

0       (OR)      0        0        1 1          0         X             X    X    X
1       (OR)      0        0        1 0          0         X             X    X    X

loop (i,32)
0       (OR)      (A&gt;&gt;i&amp;1) (B&gt;&gt;i&amp;1) 1 1          0         ((A|B)&gt;&gt;i&amp;1)  X    X    X
1       (OR)      (A&gt;&gt;i&amp;1) (B&gt;&gt;i&amp;1) 1 1          0         X             X    X    X
